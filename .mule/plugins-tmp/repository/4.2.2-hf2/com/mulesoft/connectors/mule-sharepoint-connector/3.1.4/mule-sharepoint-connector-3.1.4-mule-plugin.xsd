  <xs:element xmlns="http://www.mulesoft.org/schema/mule/sharepoint" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Created-objectsType" substitutionGroup="mule:abstract-message-source" name="created-objects"></xs:element>
