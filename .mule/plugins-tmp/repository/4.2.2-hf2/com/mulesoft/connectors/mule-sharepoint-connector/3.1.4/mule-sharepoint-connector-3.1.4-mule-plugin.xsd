          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-redelivery-policy"></xs:element>
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
          <xs:element minOccurs="1" maxOccurs="1" ref="mule:scheduling-strategy"></xs:element>
        </xs:sequence>
        <xs:attribute type="xs:string" use="optional" name="since"></xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sharepoint" type="com.mulesoft.connectors.sharepoint.api.TriggersKeysApiEnumType" use="required" name="objectType">
          <xs:annotation>
            <xs:documentation>The type of objects to listen for</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="listId">
          <xs:annotation>
            <xs:documentation>This optional field is used only when listening for newly created list items</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:substitutableBoolean" use="optional" default="false" name="recursive">
          <xs:annotation>
            <xs:documentation>This optional field is used only when listening for created files or folders</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" default="/" name="path">
          <xs:annotation>
            <xs:documentation>Optional parameter used for FOLDER and FILE object types, that specifies the base path to start looking at</xs:documentation>
