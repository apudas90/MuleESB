            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="proxyConfig">
            <xs:annotation>
              <xs:documentation>Proxy configuration for the connector.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
