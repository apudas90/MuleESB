  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" substitutionGroup="mule:abstract-shared-extension" name="listener-config">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mule:abstractExtensionType">
          <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstractConnectionProvider"></xs:element>
            <xs:element minOccurs="0" maxOccurs="1" ref="mule:expiration-policy"></xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="inbound-request-authentication">
              <xs:annotation>
                <xs:documentation>Authentication method to use for verifying the incoming HTTP request.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:choice minOccurs="1" maxOccurs="1">
                  <xs:element xmlns:as2-mule4="http://www.mulesoft.org/schema/mule/as2-mule4" minOccurs="1" maxOccurs="1" ref="as2-mule4:abstract-basic-authentication-verifier"></xs:element>
                  <xs:element xmlns:as2-mule4="http://www.mulesoft.org/schema/mule/as2-mule4" minOccurs="1" maxOccurs="1" ref="as2-mule4:global-abstract-basic-authentication-verifier"></xs:element>
                </xs:choice>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="1" maxOccurs="1" name="self-config">
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension base="mule:abstractExtensionType">
                    <xs:sequence></xs:sequence>
                    <xs:attribute type="mule:expressionString" use="required" name="as2Name">
                      <xs:annotation>
                        <xs:documentation>AS2 system identifier (used for AS2-From and AS2-To headers)</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:expressionString" use="optional" name="x509Alias">
                      <xs:annotation>
                        <xs:documentation>Certificate and/or private key alias in keystore (required if using signing or encryption)</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:expressionString" use="required" name="email">
                      <xs:annotation>
                        <xs:documentation>Email address used as HTTP 'From' header when sending</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="1" maxOccurs="1" name="partner-config">
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension base="mule:abstractExtensionType">
                    <xs:sequence></xs:sequence>
                    <xs:attribute type="mule:expressionString" use="required" name="as2Name">
                      <xs:annotation>
                        <xs:documentation>AS2 system identifier (used for AS2-From and AS2-To headers)</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:expressionString" use="optional" name="x509Alias">
                      <xs:annotation>
                        <xs:documentation>Certificate and/or private key alias in keystore (required if using signing or encryption)</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:expressionString" use="required" name="email">
                      <xs:annotation>
                        <xs:documentation>Email address used as HTTP 'From' header when sending</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="1" maxOccurs="1" name="key-store-config">
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension base="mule:abstractExtensionType">
                    <xs:sequence></xs:sequence>
                    <xs:attribute type="mule:expressionString" use="required" name="keystorePassword">
                      <xs:annotation>
                        <xs:documentation>The password of the provided keystore.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:expressionString" use="required" name="keystorePath">
                      <xs:annotation>
                        <xs:documentation>The Absolute or relative path to the keystore containing certificates and private keys for self(this application) and the partner.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute type="mule:expressionString" use="optional" name="privateKeyPassword">
                      <xs:annotation>
                        <xs:documentation>If the private key to be used for signing/encryption is password protected, please give that value here.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute type="xs:string" use="required" name="httpListenerConfig">
            <xs:annotation>
              <xs:documentation>Reference to global HTTP Listener definition for our service endpoint used by partner for both action and signal messages.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="required" name="name">
            <xs:annotation>
              <xs:documentation>The identifier of this element used to reference it in other components</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" substitutionGroup="mule:abstractConnectionProvider" name="async-mdn-connection-provider-connection">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mule:abstractConnectionProviderType">
          <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element minOccurs="0" maxOccurs="1" ref="mule:reconnection"></xs:element>
            <xs:element xmlns:tls="http://www.mulesoft.org/schema/mule/tls" minOccurs="0" maxOccurs="1" ref="tls:context"></xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="proxy-config">
              <xs:annotation>
                <xs:documentation>Reusable configuration element for outbound connections through a proxy. A proxy element must define a host name and a port attributes, and optionally can define a username and a password.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:choice minOccurs="1" maxOccurs="1">
                  <xs:element xmlns:as2-mule4="http://www.mulesoft.org/schema/mule/as2-mule4" minOccurs="1" maxOccurs="1" ref="as2-mule4:abstract-http-proxy-config"></xs:element>
                  <xs:element xmlns:as2-mule4="http://www.mulesoft.org/schema/mule/as2-mule4" minOccurs="1" maxOccurs="1" ref="as2-mule4:global-abstract-http-proxy-config"></xs:element>
                </xs:choice>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="authentication">
              <xs:annotation>
                <xs:documentation>Authentication method to use for the HTTP request. Not to be used!</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension xmlns:as2-mule4="http://www.mulesoft.org/schema/mule/as2-mule4" base="as2-mule4:org.mule.extension.http.api.request.authentication.BasicAuthentication"></xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute type="xs:string" use="optional" name="tlsContext"></xs:attribute>
          <xs:attribute type="mule:substitutableBoolean" use="optional" default="false" name="usePersistentConnections">
            <xs:annotation>
              <xs:documentation>If false, each connection will be closed after the first request is completed.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:substitutableInt" use="optional" default="30" name="connectionTimeout">
            <xs:annotation>
              <xs:documentation>The timeout for establishing connections to the remote service. This value is qualified by the #connectionTimeoutUnits</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/as2-mule4" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="SECONDS" name="connectionTimeoutUnits">
            <xs:annotation>
              <xs:documentation>A time unit which qualifies the #connectionTimeout</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:substitutableInt" use="optional" default="-1" name="maxConnections">
            <xs:annotation>
              <xs:documentation>The maximum number of outbound connections that will be kept open at the same time. By default the number of connections is unlimited.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="mule:substitutableInt" use="optional" default="30" name="connectionIdleTimeout">
            <xs:annotation>
              <xs:documentation>A timeout for how long a connection can remain idle before it is closed. The value of this attribute is only used when persistent connections are enabled. This value is qualified bY the #connectionIdleTimeoutUnits</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/as2-mule4" type="java.util.concurrent.TimeUnitEnumType" use="optional" default="MINUTES" name="connectionIdleTimeoutUnits">
            <xs:annotation>
              <xs:documentation>A time unit which qualifies the #connectionIdleTimeoutUnits</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="proxyConfig">
            <xs:annotation>
              <xs:documentation>Reusable configuration element for outbound connections through a proxy. A proxy element must define a host name and a port attributes, and optionally can define a username and a password.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="authentication">
            <xs:annotation>
              <xs:documentation>Authentication method to use for the HTTP request. Not to be used!</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="org.mule.extension.http.api.request.authentication.BasicAuthentication">
    <xs:annotation>
      <xs:documentation>Authentication method to use for the HTTP request. Not to be used!</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractExtensionType">
        <xs:attribute type="mule:expressionString" use="required" name="username"></xs:attribute>
        <xs:attribute type="mule:expressionString" use="required" name="password"></xs:attribute>
        <xs:attribute type="mule:expressionBoolean" use="optional" default="true" name="preemptive"></xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/as2-mule4" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="As2-listenerType" substitutionGroup="mule:abstract-message-source" name="as2-listener">
    <xs:annotation>
      <xs:documentation>Represents a listener for AS2 sender requests</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="As2-listenerType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageSourceType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-redelivery-policy"></xs:element>
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-byte-streaming-strategy"></xs:element>
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
        </xs:sequence>
        <xs:attribute type="xs:string" use="optional" default="/" name="path">
          <xs:annotation>
            <xs:documentation>This path will be appended to any path set in the listener configuration.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:substitutableInt" use="optional" default="0" name="connectionTimeout">
          <xs:annotation>
            <xs:documentation>An override for the timeout for establishing connections to the remote service as milliseconds. The default of 0 means this value will not be used to override the configuration.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="outputMimeType">
          <xs:annotation>
            <xs:documentation>The mime type of the payload that this operation outputs.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:expressionString" use="optional" name="outputEncoding">
          <xs:annotation>
            <xs:documentation>The encoding of the payload that this operation outputs.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:substitutableBoolean" use="optional" name="primaryNodeOnly">
          <xs:annotation>
            <xs:documentation>Whether this source should only be executed on the primary node when runnning in Cluster</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" substitutionGroup="mule:abstract-shared-extension" name="mdn-listener-config">
